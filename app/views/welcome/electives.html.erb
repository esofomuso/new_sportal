<div class="span3">
	<%= render :partial => '/shared/user_details' %>
</div>
<div class="span9">
  <%= bootstrap_flash %>
  <div>
	  <div>
	    <div> <h3><u>Electives 2013 - 2014</u></h3></div>
			<br/>
			<hr/>
			<br/>
	    <div class='contentWrapper ng-cloak'>
	   	  <div class='content'>
	   	  <div>
	   	    <h4>Available Electives</h4>
	   	  </div>
	      <table style='width: 90%;'>
	      	<tr><th>Electives up for grabs</th><th> Drag and Drop Electives Below </th></tr>
	        <tr>
	          <td id="drag-target-three" style='width: 15%;'>
	          	<div id="drag-target-three" name="avail[electives]" class='avail_electives' >
		            <ul id="drag-elements-three">
		              <% active_electives.each do |e| %>
	              		<li draggable="true" class="electable btn btn-info" style='width: 80%;'>
		              		<div class="caption" elective_id='<%= e.id %>'>
		              			<%= e.name %>
		              		</div>
	              		</li>
		              <% end %>
		            </ul>
		         </div>
	          </td>
	          <td id="drop-target-three" style='width: 15%;'>
	            <div id="drop-target-three" name="chosen[electives]" class='chosen_electives' >
	            	<ul id="drop-elements-three">
	                	<% selected_electives.each do |e| %>
	                		<li draggable="true" class="elected btn btn-info" style='width: 80%;'>
		                		<div class="caption" elective_id='<%= e.id %>'>
		                			<%= e.name %>
		                		</div>
	                		</li>
	                 	<% end %>
	            	</ul>
	            </div>
	          </td>
	        </tr>
	      </table>
	    </div>
	  </div>
	</div>
</div>

